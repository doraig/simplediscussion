<h2>Topics</h2>
<div >
  <div>
    <ul class="collection">
    <%= for topic <- @topics do %>
    <li class="collection-item">
      <%= topic.title%>
    </li>
    <% end %>
    </ul>
  </div>
  <div class="right-align">
    <%= link to: topic_path(@conn, :new), class: "btn-floating btn-large waves-effect waves-light red" do %>
      <i class="material-icons">add</i>
    <% end %>
  </div>
</div>
